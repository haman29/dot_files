[user]
    name = Kyohei Hamada
    email = kyohei.hamada@gmail.com
[alias]
    co = checkout
    ci = commit
    st = status
[core]
    editor = vim
    excludesfile = /Users/hamada/.gitignore_global
[push]
    default = tracking
[pull]
    default = tracking
[difftool "sourcetree"]
    cmd = opendiff \"$LOCAL\" \"$REMOTE\"
    path = 
[mergetool "sourcetree"]
    cmd = /Applications/SourceTree.app/Contents/Resources/opendiff-w.sh \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
    trustExitCode = true
[color]
	diff = auto
	status = auto
	branch = auto
